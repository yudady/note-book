<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="450.0" prefWidth="620.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.tommy.fx06.view.BirthdayStatisticsController">
    <children>
        <BarChart fx:id="barChart" layoutY="25.0" legendVisible="false" prefHeight="450.0" prefWidth="620.0"
                  AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
            <xAxis>
                <CategoryAxis side="BOTTOM" tickLabelRotation="-90.0" fx:id="xAxis"/>
            </xAxis>
            <yAxis>
                <NumberAxis minorTickCount="0" minorTickVisible="false" side="LEFT" tickUnit="1.0"/>
            </yAxis>
        </BarChart>
    </children>
</AnchorPane>
